<?xml version="1.0"?>
<schema version="0.3">
  <table name="btContentScheduler">
	<field name="scID" type="I">
		<key />
		<unsigned />
		<autoincrement/>
	</field>
	<field name="title" type="C" size="100">
	</field>
	<field name="ctID" type="I">
	</field>
	<field name="pubdatetime" type="T"/>
	<field name="content" type="X2"/>
	<field name="fID" type="I">
		<unsigned />
		<default value="0" />
	</field>
	<field name="link"  type="C" size="255">
	</field>
	<field name="newTab" type="L">
		<default value="0" />
	</field>
	<field name="linkText" type="C" size="255">
	</field>
	<field name="caption" type="C" size="255">
	</field>
  </table>
  <table name="btContentCategories">
	<field name="ctID" type="I">
		<key />
		<unsigned />
		<autoincrement/>
	</field>
	<field name="category" type="C" size="100">
	</field>
  </table>
</schema>